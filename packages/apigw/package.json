{
  "name": "@fearlessfara/apigw-vela",
  "version": "0.1.2",
  "description": "API Gateway VTL adapter - AWS API Gateway specific VTL functionality",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
