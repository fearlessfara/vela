{
  "body": "$input.body",
  "bodyBytes": "$util.base64Encode($input.body)",
  "json": $input.json(),
  "jsonPath": $input.json('$'),
  "jsonPathTitle": "$input.json('$.title')",
  "jsonPathContent": "$input.json('$.content')",
  "jsonPathTags": $input.json('$.tags'),
  "jsonPathFirstTag": "$input.json('$.tags[0]')",
  "jsonPathSecondTag": "$input.json('$.tags[1]')",
  "jsonPathAuthor": "$input.json('$.metadata.author')",
  "jsonPathVersion": $input.json('$.metadata.version'),
  "params": $input.params(),
  "paramUserId": "$input.params('userId')",
  "paramInclude": "$input.params('include')",
  "paramFormat": "$input.params('format')",
  "paramVersion": "$input.params('version')",
  "paramContentType": "$input.params('content-type')",
  "paramAuthorization": "$input.params('authorization')",
  "paramXRequestId": "$input.params('x-request-id')",
  "headers": $input.headers(),
  "headerContentType": "$input.header('Content-Type')",
  "headerAuthorization": "$input.header('Authorization')",
  "headerXRequestId": "$input.header('X-Request-ID')",
  "headerUserAgent": "$input.header('User-Agent')",
  "paths": $input.paths(),
  "pathUserId": "$input.path('userId')",
  "querystrings": $input.querystrings(),
  "querystringInclude": "$input.querystring('include')",
  "querystringFormat": "$input.querystring('format')",
  "querystringVersion": "$input.querystring('version')"
}
