{
  "name": "@fearlessfara/velocits",
  "version": "1.0.1",
  "description": "TypeScript Apache Velocity engine with 1:1 Java compatibility",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "browser": "dist-browser/browser.js",
  "unpkg": "dist-browser/velocits.umd.js",
  "jsdelivr": "dist-browser/velocits.umd.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist/**",
    "dist-browser/**",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:node && npm run build:browser && npm run build:umd",
    "build:node": "tsc && tsc -p tools/tsconfig.json && tsc -p tests/tsconfig.json && tsc -p examples/tsconfig.json",
    "build:browser": "tsc -p tsconfig.browser.json",
    "build:umd": "rollup -c",
    "build:tools": "tsc -p tools/tsconfig.json",
    "test": "npm run test:unit && npm run test:velocity",
    "test:unit": "npm run build:node && node dist/tests/unit/variables-and-interpolation.test.js && node dist/tests/unit/directives.test.js && node dist/tests/unit/expressions-and-operators.test.js && node dist/tests/unit/utility-registration.test.js",
    "test:velocity": "npm run build:node && node dist/tools/compare-velocity/harness.js",
    "test:velocity:single": "npm run build:node && node dist/tools/compare-velocity/harness.js",
    "add-test": "npm run build:tools && node dist/tools/add-test.js",
    "examples": "npm run build:node && node dist/examples/01-basic-usage.js && node dist/examples/02-utility-registration.js && node dist/examples/03-email-template.js && node dist/examples/04-report-generator.js && node dist/examples/05-config-generator.js",
    "example:basic": "npm run build:node && node dist/examples/01-basic-usage.js",
    "example:utils": "npm run build:node && node dist/examples/02-utility-registration.js",
    "example:email": "npm run build:node && node dist/examples/03-email-template.js",
    "example:report": "npm run build:node && node dist/examples/04-report-generator.js",
    "example:config": "npm run build:node && node dist/examples/05-config-generator.js",
    "dev": "tsc --watch",
    "clean": "rm -rf dist dist-browser"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/node": "^22.19.1",
    "rollup": "^4.50.2",
    "semantic-release": "^24.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "chevrotain": "^11.0.3"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "keywords": [
    "vtl",
    "velocity",
    "template",
    "template-engine",
    "chevrotain",
    "parser",
    "browser",
    "node",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fearlessfara/vela.git"
  },
  "homepage": "https://github.com/fearlessfara/vela#readme",
  "bugs": {
    "url": "https://github.com/fearlessfara/vela/issues"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "glob": ">=11.0.4",
    "brace-expansion": ">=2.0.2",
    "ip": ">=2.0.1"
  }
}
